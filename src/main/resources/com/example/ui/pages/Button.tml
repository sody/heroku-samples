<html t:type="layout" title="message:page.button.title"
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_1_0.xsd"
		>

<iframe src="${hiddenPageLink}" width="100%" frameborder="0" marginheight="0" marginwidth="0">
</iframe>

<script type="text/javascript">
	jQuery(function($) {
		$("iframe").load(function(){
			$(this).height($(this).contents().find('html').height() + 20);
		});
	});
</script>

</html>
